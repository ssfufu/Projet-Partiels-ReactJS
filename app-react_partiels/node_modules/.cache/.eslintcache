lphabet.split('')\n        while(resultats.length < taille){\n            const lettre = toutesLesLettres.shift()\n            resultats.push(lettre)\n        }\n        return resultats\n    }\n\n    feedback(lettre) {\n        const{selection} = this.state\n        return selection.includes(lettre)\n    }\n\n    clickH = lettre => {\n        const {selection, stateDuJeu} = this.state\n        if(stateDuJeu == \"en cours\"){\n            this.setState({selection: [...selection, lettre]}, this.stateDuJeu)\n        }\n    }\n\n    stateDuJeu = () =>{\n        const {lettres, selection} = this.state\n        const tests = 8 - this.essai()\n        const rechercheMot = lettres.filter(element => selection.includes(element)).length === lettres.length\n        if(tests > 0 && rechercheMot){\n            this.setState({stateDuJeu : \"gagnée avec succès\"})\n        } else if (tests > 0){\n            return\n        } else {\n            this.setState({stateDuJeu : \"perdu avec succès!!\"})\n        }\n    }\n\n    essai = () => {\n        const {lettres, selection} = this.state\n        return selection.filter(element => !lettres.includes(element)).length\n    }\n\n    nouvellePartie = () => {\n        this.setState({selection:[], lettres: this.generationMots(), stateDuJeu: \"en cours\"})\n    }\n\n    render(){\n        const {lettres, clavier} = this.state\n\n        return (\n            <div>\n                <div>\n                    <h1>Jeu du pendu</h1>\n                    <button className=\"btn btn-info\" onClick={this.nouvellePartie}>Nouvelle partie</button>\n                </div>\n                <div>\n                    <div>\n                    { lettres.map((lettre, index) => (\n                        <Lettre\n                        lettre={lettre}\n                        feedback={this.feedback(lettre) ? \"visible\" : \"hidden\"}\n                        key={index}\n                        />\n                    ))}\n                    </div>\n                    <Compteur\n                    compteur = {this.essai()}\n                    stateDuJeu = {this.state.stateDuJeu}\n                    />\n                </div>\n\n                <div className=\"keyboard\">\n                    { clavier.map((lettre, index) => (\n                    <Clavier\n                        lettre={lettre}\n                        key={index}\n                        onClick={this.clickH}\n                        feedback={this.feedback(lettre) ? \"gray\" : \"#17a2b8\"}\n                    />\n                    ))}\n                </div>\n            </div>\n\n        )\n\n\n    }\n}\n\nexport default App;","/Users/mathieu/Desktop/Travail/Partiels/ReactJS/Projet-Partiels-ReactJS/app-react_partiels/src/Lettres/Lettres.jsx",["41"],[],"/Users/mathieu/Desktop/Travail/Partiels/ReactJS/Projet-Partiels-ReactJS/app-react_partiels/src/C/Compteur.jsx",[],[],["42","43"],"/Users/mathieu/Desktop/Travail/Partiels/ReactJS/Projet-Partiels-ReactJS/app-react_partiels/src/B/Clavier.jsx",["44"],[],"import React from 'react'\nimport PropTypes from 'prop-types'\n\n\nconst Clavier = ({ lettre, onClick, feedback}) => (\n  <div style={{backgroundColor: `${feedback}`, display: 'inline', fontSize:'2.5em',paddingBottom:'50px', marginRight:'10px'}}className={\"lettre\"} onClick={() => onClick(lettre)}>\n\n    <span>\n      \n    {lettre}\n    </span>\n  </div>\n)\n\nexport default Clavier\n","/Users/mathieu/Desktop/Travail/Partiels/ReactJS/Projet-Partiels-ReactJS/app-react_partiels/src/registerServiceWorker.js",[],[],{"ruleId":"45","severity":1,"message":"46","line":50,"column":23,"nodeType":"47","messageId":"48","endLine":50,"endColumn":25},{"ruleId":"49","severity":1,"message":"50","line":2,"column":8,"nodeType":"51","messageId":"52","endLine":2,"endColumn":17},{"ruleId":"53","replacedBy":"54"},{"ruleId":"55","replacedBy":"56"},{"ruleId":"49","severity":1,"message":"50","line":2,"column":8,"nodeType":"51","messageId":"52","endLine":2,"endCo